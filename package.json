{
  "name": "@architect/destroy",
  "version": "6.0.0",
  "description": "Destroy projects created with Architect",
  "main": "src/index.js",
  "bin": {
    "arc-destroy": "src/cli.js"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "test": "npm run lint && npm run coverage",
    "test:nolint": "npm run coverage",
    "test:unit": "cross-env tape 'test/unit/**/*-test.js'",
    "coverage": "cross-env nyc --reporter=lcov --reporter=text npm run test:unit",
    "test:integration": "cross-env tape test/integration/**/*-test.js",
    "rc": "npm version prerelease --preid RC"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/architect/destroy.git"
  },
  "dependencies": {
    "@architect/inventory": "~6.0.0",
    "@architect/utils": "~6.0.0",
    "@aws-lite/client": "^0.23.2",
    "@aws-lite/cloudformation": "^0.1.3",
    "@aws-lite/cloudwatch-logs": "^0.1.0",
    "@aws-lite/s3": "^0.2.6",
    "@aws-lite/ssm": "^0.2.3"
  },
  "devDependencies": {
    "@architect/deploy": "~7.0.0",
    "@architect/eslint-config": "~3.0.0",
    "cross-env": "~10.1.0",
    "eslint": "~9.39.1",
    "nyc": "~17.1.0",
    "proxyquire": "~2.1.3",
    "tape": "~5.9.0"
  }
}
